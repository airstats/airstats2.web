{
  "base": "semantic",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src\\site\\",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../src/semantic/",
      "uncompressed": "../src/semantic/components/",
      "compressed": "../src/semantic/components/",
      "themes": "../src/semantic/themes/"
    },
    "clean": "dist/"
  },
  "permission": 744,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "header",
    "icon",
    "image",
    "input",
    "label",
    "list",
    "loader",
    "form",
    "grid",
    "menu",
    "message",
    "table",
    "item",
    "accordion",
    "segment",
    "checkbox",
    "dropdown",
    "modal",
    "nag",
    "popup",
    "progress",
    "search",
    "shape",
    "tab",
    "api",
    "form",
    "state",
    "visibility"
  ],
  "version": "2.2.11"
}
