/*******************************
    User Variable Overrides
*******************************/

@dropdownItemBackground: @darkblue;
@dropdownHoveredItemBackground: @blue;
@dropdownItemColor: @white;
@dropdownHoveredItemColor: @white;
@borderRadius: 0;
@invertedBackground: linear-gradient(#002F5D, #002F5D 40%, #021624);
@invertedHoverBackground: @invertedBackground;
@invertedHoverColor: @white;
@invertedHoverBorderBottomColor: @orange;
@dropdownMenuDistance: 3px;
